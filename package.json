{
	"name": "ressut",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"preinstall": "pnpm install ./wasm/pkg",
		"dev": "pnpm wasm && vite --cors --host --force",
		"check": "tsc",
		"vite:build": "tsc && vite build --clearScreen false",
		"preview": "vite preview",
		"test": "vitest run",
		"coverage": "vitest run --coverage",
		"test:watch": "vitest",
		"app": "tauri",
		"app:dev": "tauri dev",
		"app:build": "tauri build",
		"app:devbuild": "tauri build --debug",
		"wasm": "wasm-pack build ./wasm --dev --target web",
		"wasm:test": "wasm-pack test --chrome ./wasm",
		"wasm:release": "wasm-pack build ./wasm --target web --release",
		"build": "pnpm wasm && pnpm vite:build",
		"release": "pnpm wasm:release && pnpm vite:build",
		"format": "prettier --config prettier.config.js --write {src,store,test,database}/**/*.{js,scss,tsx,ts,json,yml,yaml,html}"
	},
	"dependencies": {
		"@emotion/react": "^11.10.4",
		"@emotion/styled": "^11.10.4",
		"@reduxjs/toolkit": "^1.8.6",
		"@tauri-apps/api": "^1.1.0",
		"crypto-js": "^4.1.1",
		"evergreen-ui": "^6.10.4",
		"lodash-es": "^4.17.21",
		"lowdb": "^3.0.0",
		"pkg": "link:wasm/pkg",
		"re-resizable": "^6.9.9",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-hot-toast": "^2.4.0",
		"react-redux": "^7.2.9",
		"validator": "^13.7.0",
		"wasm": "link:wasm/pkg",
		"xss": "^1.0.14"
	},
	"devDependencies": {
		"@babel/core": "^7.19.6",
		"@tauri-apps/cli": "^1.1.1",
		"@types/crypto-js": "^4.1.1",
		"@types/lodash": "^4.14.186",
		"@types/lodash-es": "^4.17.6",
		"@types/node": "^17.0.45",
		"@types/react": "^17.0.50",
		"@types/react-dom": "^17.0.17",
		"@types/react-redux": "^7.1.24",
		"@types/styled-components": "^5.1.26",
		"@types/validator": "^13.7.8",
		"@types/ws": "^8.5.3",
		"@vitejs/plugin-react": "^1.3.2",
		"c8": "^7.12.0",
		"happy-dom": "^2.55.0",
		"prettier": "^2.7.1",
		"react-is": "^17.0.2",
		"rollup": "^2.79.1",
		"rollup-plugin-visualizer": "^5.8.3",
		"sass": "^1.55.0",
		"typescript": "^4.8.4",
		"vite": "^2.9.15",
		"vite-plugin-banner": "^0.2.0",
		"vitest": "^0.10.5"
	}
}
