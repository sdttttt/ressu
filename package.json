{
  "name": "ressut",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "preinstall": "pnpm install ./wasm/pkg",
    "dev": "pnpm wasm && vite --cors --host --force",
    "check": "tsc",
    "vite:build": "tsc && vite build --clearScreen false",
    "preview": "vite preview",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "app": "tauri",
    "app:dev": "tauri dev",
    "app:build": "tauri build",
    "app:devbuild": "tauri build --debug",
    "wasm": "wasm-pack build ./wasm --dev --target web",
    "wasm:test": "wasm-pack test --chrome ./wasm",
    "wasm:release": "wasm-pack build ./wasm --target web --release",
    "build": "pnpm wasm && pnpm vite:build",
    "release": "pnpm wasm:release && pnpm vite:build",
    "format": "prettier --config prettier.config.js --write {src,store,test,database}/**/*.{js,scss,tsx,ts,json,yml,yaml,html}"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@reduxjs/toolkit": "^1.8.1",
    "@tauri-apps/api": "^1.0.0-rc.3",
    "crypto-js": "^4.1.1",
    "evergreen-ui": "^6.9.9",
    "lodash-es": "^4.17.21",
    "lowdb": "^3.0.0",
    "pkg": "link:wasm/pkg",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.8",
    "react-toastify": "^8.2.0",
    "validator": "^13.7.0",
    "wasm": "link:wasm/pkg"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@tauri-apps/cli": "^1.0.0-rc.10",
    "@types/crypto-js": "^4.1.1",
    "@types/lodash": "^4.14.181",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.14",
    "@types/react-redux": "^7.1.24",
    "@types/styled-components": "^5.1.24",
    "@types/validator": "^13.7.2",
    "@types/ws": "^8.5.3",
    "@vitejs/plugin-react": "^1.3.0",
    "c8": "^7.11.2",
    "happy-dom": "^2.55.0",
    "prettier": "^2.6.2",
    "react-is": "^17.0.0",
    "rollup": "^2.70.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.49.11",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vite-plugin-banner": "^0.2.0",
    "vitest": "^0.10.0"
  }
}
