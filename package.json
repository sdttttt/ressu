{
  "name": "ressut",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "pnpm wasm && vite",
    "vite:build": "tsc && vite build --clearScreen false",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "app": "tauri",
    "app:dev": "tauri dev",
    "app:build": "tauri build",
    "wasm": "wasm-pack build ./wasm --dev --target web",
    "wasm:release": "wasm-pack build ./wasm --target web --release",
    "build": "pnpm wasm:release && pnpm vite:build",
    "format": "prettier --config prettier.config.js --write {src,redux,test}/**/*.{js,scss,tsx,ts,json,yml,yaml,html}"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@tauri-apps/api": "^1.0.0-rc.3",
    "evergreen-ui": "^6.9.5",
    "lodash": "^4.17.21",
    "lowdb": "^3.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.8",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "wasm": "link:wasm/pkg"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@tauri-apps/cli": "^1.0.0-rc.8",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.14",
    "@types/styled-components": "^5.1.24",
    "@types/ws": "^8.5.3",
    "@vitejs/plugin-react": "^1.3.0",
    "happy-dom": "^2.55.0",
    "prettier": "^2.6.2",
    "react-is": "^17.0.0",
    "rollup": "^2.70.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.49.11",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vitest": "^0.8.4"
  }
}
